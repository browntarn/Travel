<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user001">
	<select id="selectUser001List" resultType="hashmap">
		SELECT ID
		      ,MNAME
		      ,ADDRESS
		      ,ZIP
		      ,TEL
		      ,EMAIL
		      ,CLEVEL
		      ,CDATE
		      ,USE_FLAG
		      ,EN_LAN
		 FROM P_USER_INFO					
	</select>
</mapper>